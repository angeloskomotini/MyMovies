  use_frameworks!

  pod 'AFNNetworking'

  pod 'SDWebImage'

  pod 'Alamofire'

  pod 'SwiftyJSON'

  pod 'Cosmos'

  target 'MyMoviesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MyMoviesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
